package DSproject;

public class BST<T> {
	public String key;
    public T data;
    public BST<T> left, right;

    public BST(String k, T val) {
            key = k;
            data = val;
            left = right = null;
    }

    public BST(String k, T val, BST<T> l, BST<T> r) {
            key = k;
            data = val;
            left = l;
            right = r;
    }


}


